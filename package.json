{
  "name": "blog.questionable.services",
  "version": "1.0.0",
  "description": "The Cloudflare Worker for blog.questionable.services",
  "author": "Matt Silverlock <matt@eatsleeprepeat.net>",
  "url": "https://blog.questionable.services/",
  "license": "MIT",
  "main": "workers/index.ts",
  "scripts": {
    "build": "bundle install && tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "deploy": "wrangler deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elithrar/elithrar.github.io.git"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "blog",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/elithrar/elithrar.github.io/issues"
  },
  "homepage": "https://github.com/elithrar/elithrar.github.io#readme",
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "printWidth": 120,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "dependencies": {
    "wrangler": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250109.0",
    "typescript": "^5.7.0"
  }
